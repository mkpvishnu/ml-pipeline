{
    "canvas_id": "ingestion_canvas_001",
    "canvas_config": [
        {
            "module_id": "download_001",
            "identifier": "s3_downloader",
            "user_config": {
                "s3_link": "s3://general-vishnu/facts.txt",
                "access": "public"
            }
        },
        {
            "module_id": "process_001",
            "identifier": "document_processor",
            "user_config": {
                "chunk_size": 1000,
                "chunk_overlap": 200,
                "splitting_strategy": "recursive"
            }
        },
        {
            "module_id": "process_002",
            "identifier": "document_preprocessor",
            "user_config": {
                "stop_words":[".", "\n"]
            }
        },
        {
            "module_id": "embed_001",
            "identifier": "embeddings_generator",
            "user_config": {
                "model": "all-minilm-l6-v2",
                "batch_size": 100,
                "dimensions": 1536
            }
        },
        {
            "module_id": "store_001",
            "identifier": "vector_store",
            "user_config": {
                "database": "pinecone",
                "index_name": "documents",
                "metric": "cosine",
                "namespace": "pdf_docs"
            }
        }
    ]
}